#include<stdio.h>
int  main()
{
    int i,n,a[100],boy;
    printf("Enter the number of elements:\n");
    scanf("%d",&n) ;
    printf("Enter the elements\n");
    for(i=0;i<n;i++)
    {
      scanf("%d",&a[i]);
    }
    boy=a[0];
    for(i=1;i<n;i++)
    {
      if(boy<a[i])
        {
          boy=a[i];
        }
    }
    printf("Largest of %d elements in an array = %d",n,boy);
return 0;
}
